{
	"name": "sseeeedd",
	"version": "0.4.8",
	"description": "Sow sseeeedd and see your front-end grow",
	"repository": {
		"type": "git",
		"url": "git@github.com:ffoodd/sseeeedd.git"
	},
	"author": {
		"name": "GaÃ«l Poupard",
		"url": "https://www.ffoodd.fr",
		"email": "gael+npm@ffoodd.fr"
	},
	"license": "MIT",
	"keywords": [
		"template",
		"accessibility",
		"front-end",
		"starter"
	],
	"style": "dist/sseeeedd.min.css",
	"sass": "src/sseeeedd.scss",
	"main": "dist/js/script.min.js",
	"scripts": {
		"pretest": "npm run build && npm run build:docs",
		"test": "concurrently \"npm:test:*\"",
		"test:html": "html-validate \"docs/*.html\"",
		"test:a11y": "pa11y-ci 'docs/*.html' -c .pa11yci.json",
		"prestart": "npm run build && npm run build:docs",
		"start": "concurrently --kill-others \"npm:watch\" \"npm:browser:start\"",
		"browser:start": "servor docs --browse --reload --secure",
		"build": "sass src/:dist/ --style expanded --source-map",
		"postbuild": "postcss dist/{sseeeedd,sseeeedd-print}.css --dir dist/ --ext .min.css -u autoprefixer cssnano -m",
		"prebuild:docs": "shx rm -rf docs/* && shx mkdir docs/img/ docs/js/",
		"build:docs": "concurrently \"npm:build:docs:*\"",
		"postbuild:docs": "shx cp -r _site/{favicon.svg,humans.txt} docs/ && shx mkdir docs/css && shx cp -r dist/*.min.{css,css.map} docs/css",
		"build:docs:html": "nunjucks *.njk _site/datas/deck.json -p _site/templates -o docs",
		"prebuild:docs:svg": "shx rm -rf docs/img/symbol.svg",
		"build:docs:svg": "svgo _site/img/**/*.svg -o docs/img/",
		"postbuild:docs:svg": "svg-sprite docs/img/*.svg --config .svgsprite.json",
		"build:docs:js": "terser _site/js/script.js -o docs/js/script.min.js -c -m --source-map && terser _site/js/prism.js -o docs/js/prism.min.js -c -m --source-map",
		"build:docs:json": "",
		"watch": "concurrently --kill-others \"npm:watch:*\"",
		"watch:html": "nodemon --watch _site/templates --ext njk --exec \"npm run build:docs:html\"",
		"watch:src": "nodemon --watch src/ --ext scss --exec \"npm run build && shx cp -r dist/*.min.{css,css.map} docs/css\"",
		"watch:svg": "nodemon --watch _site/img --ext svg --exec \"npm run build:docs:svg\"",
		"watch:js": "nodemon --watch _site/js --ext js --exec \"npm run build:docs:js\"",
		"watch:data": "nodemon --watch _site/datas --ext json --exec \"npm run build:docs:json\"",
		"lint": "concurrently \"npm:lint:*\"",
		"lint:scss": "stylelint src/**/*.scss",
		"lint:js": "eslint _site/js/**/*.js"
	},
	"ignore": [
		"node_modules"
	],
	"devDependencies": {
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
		"cssnano": "^6.0.1",
		"eslint": "^8.55.0",
		"html-validate": "^8.7.3",
		"nodemon": "^3.0.2",
		"nunjucks-cli": "^1.0.0",
		"pa11y-ci": "^3.1.0",
		"pa11y-ci-reporter-html": "^6.0.2",
		"postcss": "^8.4.32",
		"postcss-cli": "^11.0.0",
		"sass": "^1.69.5",
		"servor": "^4.0.2",
		"shx": "^0.3.4",
		"stylelint": "^15.11.0",
		"stylelint-config-recommended-scss": "^13.1.0",
		"stylelint-scss": "^5.3.1",
		"svg-sprite": "^2.0.2",
		"svgo": "^3.0.5",
		"terser": "^5.24.0"
	}
}
